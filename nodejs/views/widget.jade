- var width = 1100
- var height = 500

link(rel="stylesheet", href="/css/widgets.css")

block scripts
    script(type="text/javascript", src="/js/jquery.js")
    script(type="text/javascript", src="/js/iris.js")
    script(type="text/javascript", src="#{filename}")
    script(type="text/javascript")
        $(function () {
            Iris.Widget["#{name}"]
                .create(document.getElementById("#{name}"));
        });

div(id="#{name}", style="position: relative; min-width:500px")
